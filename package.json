{
  "name": "printer-module",
  "version": "0.0.1",
  "description": "Printer Module",
  "type": "commonjs",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/dotenv": "^8.2.0",
    "@types/escpos": "^2.5.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/qr-image": "^3.2.6",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "shelljs": "^0.8.5",
    "ts-node": "10.9.1",
    "tslint": "^6.1.3",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "i": "^0.3.7",
    "morgan": "^1.10.0",
    "npm": "^9.8.1",
    "qr-image": "^3.2.0",
    "escpos": "^3.0.0-alpha.6",
    "escpos-usb": "^3.0.0-alpha.4",
    "reflect-metadata": "^0.1.13",
    "socket.io-client": "^4.7.2",
    "usb": "^1.9.2",
    "sqlite3": "^5.1.6",
    "typeorm": "0.3.17",
    "winston": "^3.10.0"
  },
  "scripts": {
    "tsc": "tsc -p .",
    "clean": "rimraf build/*",
    "postinstall": "npm run build",
    "dev": "ts-node src/printer.ts",
    "build": "npm-run-all clean tsc copy-files",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "copy-files": "copyfiles -u 1 src/**/*{.js,.json,.jpg,.png} build/src",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/typeorm-config.ts",
    "migration:run": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:run",
    "start": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 src/printer.js",
    "migration:revert": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:revert"
  }
}
